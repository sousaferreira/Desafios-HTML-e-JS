    <script>
        function abre1(){
            window.open("http://www.google.com")
        
        }
        function  abre2(){
            window.open("http://www.google.com", "simples ", "width=350, height=400");

        }
        function abre3(){
            window.open("http://www.google.com", "simples ", "menubar, scrollbars, width=350, height=400");
        }
    </script>
    <form>
        <p>Dê um clique no botão para abrir uma janela simples:</p>
        <input type="button" name="abrir" value="Janela1" onclick="abre1()"></input>

        <input type="button" name="abrir" value="Janela2" onclick="abre2()"></input>

        <input type="button" name="abrir" value="Janela3" onclick="abre3()"></input>
    </form>
